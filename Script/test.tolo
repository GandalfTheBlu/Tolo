
struct vec3
{
	float x
	float y
	float z
}

struct Transform
{
	vec3 pos
	vec3 forward
	char id
}

float main(vec3 pos, vec3 forward)
{
	char id = 'x'
	Transform t = Transform(pos, forward, id)
	
	print(t.pos.x)
	print('\n')
	print(t.forward.z)
	print('\n')
	print(t.id)
	
	print('\n')
	t.pos.x = 1337.0
	print(t.pos.x)
	
	return 1.0
}