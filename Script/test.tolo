
struct vec3
{
	float x
	float y
	float z
}

vec3 v_add(vec3 a, vec3 b)
{
	return vec3(add(a.x, b.x), add(a.y, b.y), add(a.z, b.z))
}

float main(vec3 a, vec3 b)
{
	vec3 c = v_add(a, b)
	print(c.x)
	print('\n')
	print(c.y)
	print('\n')
	print(c.z)
	print('\n')
	
	c.x = 1337.0
	print(c.x)
	
	return 1.0
}