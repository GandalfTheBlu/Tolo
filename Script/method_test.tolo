#include <memory>
#include <io>

struct Animal
{
    int age;
};

int Animal::GetAge()
{
    return this.age;
}

struct Dog : Animal
{};

struct Cat : Animal
{
    int lives;
};

void main()
{
    Dog dog = Dog(12);
    Animal::ptr pDog = Animal::ptr(&dog);
    int age = pDog.GetAge();
    print(age);
    print("\n");

    Cat cat = Cat(2, 9);
    print(cat.lives);
}