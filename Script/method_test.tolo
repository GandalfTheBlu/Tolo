#include <io>

struct Obj
{
    int a;
    int b;
};

int Obj::sum()
{
    return this.a + this.b;
}

void Obj::set_a(int x)
{
    this.a = x;
}

int sum(Obj::ptr obj)
{
    return obj.a + obj.b;
}

void main()
{
    Obj a = Obj(11, 22);

    a.set_a(44);

    print_int(a.sum());
    print_str("\n");
    print_int(sum(Obj::ptr(&a)));
}