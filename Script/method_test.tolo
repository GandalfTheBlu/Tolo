#include <memory>
#include <io>

struct Obj
{
    int a;
};

void Obj::print()
{
    print("\n");
    print(this.a);
}

void Obj::print(int times)
{
    int i=0;
    while(i<times)
    {
        this.print();
        i = i + 1;
    }
}

void main()
{
    print('x');
    print(1337);
    print(3.1415);
    print("hello");

    char c = cast(12);
    print(c);

    int i = cast(3.14);
    print(i);

    Obj o = Obj(420);
    o.print();
    o.print(3);
}