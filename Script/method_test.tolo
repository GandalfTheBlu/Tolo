#include <memory>
#include <io>

void main()
{
    int i=0;
    while(i<3)
    {
        print("i: ");
        print(i);
        print("\n");

        int j=0;
        while(j<3)
        {
            if(i == 1 && j == 2)
            {
                i = i + 1;
                continue 2;
            }

            print("j: ");
            print(j);
            print("\n");

            j = j + 1;
        }

        i = i + 1;
    }
}