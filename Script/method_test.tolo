#include <memory>
#include <io>


struct vector
{
    int typeSize;
    int count;
    ptr data;
};

void vector::malloc()
{
    this.data = malloc(this.typeSize * this.count);
}

void vector::free()
{
    free(this.data);
}

ptr vector::at(int index)
{
    return this.data + this.typeSize * index;
}

void log(int i)
{
    print(i);
    print("\n");
}

void main()
{
    vector v = vector(sizeof(int), 3, nullptr);
    v.malloc();

    *v.at(0) = 11;
    *v.at(1) = 22;
    *v.at(2) = 33;

    int a = *v.at(0);
    int b = *v.at(1);
    int c = *v.at(2);

    log(a);
    log(b);
    log(c);

    v.free();
}