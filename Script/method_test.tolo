#include <memory>
#include <io>

struct Obj
{
    int a;
};

void Obj::print()
{
    print("\n");
    print(this.a);
}

void Obj::print(int times)
{
    int i=0;
    while(i<times)
    {
        this.print();
        i = i + 1;
    }
}

char operator==(Obj lhs, Obj rhs)
{
    return lhs.a == rhs.a;
}

char operator==(Obj lhs, int rhs)
{
    return lhs.a == rhs;
}

void main()
{
    print('x');
    print(1337);
    print(3.1415);
    print("hello");

    char c = cast(12);
    print(c);

    int i = cast(3.14);
    print(i);

    Obj o = Obj(420);
    o.print();
    o.print(3);

    print("\n");

    int cmp1 = cast(Obj(1) == Obj(2));
    print(cmp1);
    print("\n");

    int cmp2 = cast(Obj(1) == Obj(1));
    print(cmp2);
    print("\n");

    int cmp3 = cast(Obj(2) == 2);
    print(cmp3);
    print("\n");
}