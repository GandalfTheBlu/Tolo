#include <memory>
#include <io>

struct vec3
{
    float x;
    float y;
    float z;
};

vec3 operator*(vec3 lhs, vec3 rhs)
{
    return vec3(lhs.x * rhs.x, lhs.y * rhs.y, lhs.z * rhs.z);
}

vec3 operator*(vec3 lhs, float rhs)
{
    return vec3(lhs.x * rhs, lhs.y * rhs, lhs.z * rhs);
}

void vec3::print()
{
    print('(');
    print(this.x);
    print(',');
    print(this.y);
    print(',');
    print(this.z);
    print(')');
}

void main()
{
    vec3 v1 = vec3(1., 2., 3.);
    vec3 v2 = vec3(2., 2., 2.);
    float f1 = 3.;

    vec3 p1 = v1 * v2;
    vec3 p2 = v1 * f1;

    p1.print();
    print("\n");
    p2.print();
}