#include <memory>
#include <io>

struct Parent
{
    ptr virtual;
};

void Parent::Foo() : virtual
{
    print("Parent\n");
}

struct Child : Parent
{};

void Child::Foo() : virtual
{
    print("Child\n");
}

void main()
{
    Child c = Child();
    Parent::ptr p = Parent::ptr(&c);
    p.Foo();// prints "Child"
}