#include <memory>
#include <io>

struct Parent
{
    //ptr virtual;
};

int Parent::Foo()
{
    return 11;
}

int Parent::Bar()
{
    return 0;
}

struct Child : Parent
{};

int Child::Bar()
{
    return 1;
}

int Child::Foo()
{
    Parent::ptr base = Parent::ptr(this);
    return base.Foo() + 22;
}


void nl()
{
    print("\n");
}


void main()
{
    Child c = Child();
    
    int i = c.Bar();
    print(i);

    nl();

    int j = c.Foo();
    print(j);
}